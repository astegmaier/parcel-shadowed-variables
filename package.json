{
  "name": "parcel-shadowed-variables",
  "version": "1.0.0",
  "description": "A reproduction of an issue with parcel with shadowing a global variable within a function",
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html --no-optimize --no-source-maps",
    "serve": "http-server dist",
    "serve:raw": "http-server raw --port 8090",
    "clean": "rimraf dist .parcel-cache"
  },
  "author": "Andrew Stegmaier",
  "license": "ISC",
  "dependencies": {
    "parcel": "^2.0.1"
  },
  "devDependencies": {
    "http-server": "^14.0.0",
    "rimraf": "^3.0.2"
  }
}
